<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <!--remove preload blank theme font-->
        <remove src="fonts/opensans/light/opensans-300.woff2"/>
        <remove src="fonts/opensans/regular/opensans-400.woff2"/>
        <remove src="fonts/opensans/semibold/opensans-600.woff2"/>
        <remove src="fonts/opensans/bold/opensans-700.woff2"/>
        <remove src="fonts/Luma-Icons.woff2"/>

        <!--embed Poppins from Google fonts-->
        <link rel="preconnect" src="https://fonts.googleapis.com" src_type="url" />
        <link rel="preconnect" src="https://fonts.gstatic.com" src_type="url" />
        <link rel="stylesheet" src="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;family=Silkscreen&amp;display=swap" src_type="url" />

        <css rel="stylesheet" src="css/bootstrap.min.css" type="text/css" media="all" />
        
        <css rel="stylesheet" src="css/fonts.css" type="text/css" media="all" />
        <css rel="stylesheet" src="css/animations.css" type="text/css" media="all" />
        <css rel="stylesheet" src="css/main.css" type="text/css" media="all" />
        
    </head>
</page>
